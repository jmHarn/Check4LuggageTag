<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
	<Id>e01d8e02-8d7c-4d2a-910e-52f65e9c3f45</Id>
	<Version>1.0.0.0</Version>
	<ProviderName>HarnessIP</ProviderName>
	<DefaultLocale>en-US</DefaultLocale>
	<DisplayName DefaultValue="Check for Luggage Tag" />
	<Description DefaultValue="An add-in to check for luggage tags in email subjects before sending." />
	<IconUrl DefaultValue="https://jmharn.github.io/Check4LuggageTag/Icon16.png" />
	<HighResolutionIconUrl DefaultValue="https://jmharn.github.io/Check4LuggageTag/Icon80.png" />
	<SupportUrl DefaultValue="https://www.harnessip.com" />
	<AppDomains>
		<AppDomain>https://jmharn.github.io</AppDomain>
	</AppDomains>
	<Hosts>
		<Host Name="Mailbox" />
	</Hosts>
	<Requirements>
		<Sets DefaultMinVersion="1.1">
			<Set Name="Mailbox" />
		</Sets>
	</Requirements>
	<FormSettings>
		<Form xsi:type="ItemEdit">
			<DesktopSettings>
				<SourceLocation DefaultValue="https://jmharn.github.io/Check4LuggageTag/MessageRead.html" />
			</DesktopSettings>
		</Form>
	</FormSettings>
	<Permissions>ReadWriteItem</Permissions>
	<Rule xsi:type="RuleCollection" Mode="And">
		<Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit" />
	</Rule>
</OfficeApp>
